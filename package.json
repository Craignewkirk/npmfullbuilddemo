{
  "name": "fullbuild",
  "version": "1.0.1",
  "description": "Simple demo to show off build scripts with npm",
  "main": "index.js",
  "devDependencies": {
    "coffee-script": "^1.9.3",
    "mocha": "^2.2.5",
    "should": "^7.0.4",
    "typescript": "^1.5.3"
  },
  "scripts": {
    "start": "node index.js",
    "pretest": "npm run clean && npm run compile",
    "test": "mocha test -u bdd -R dot",
    "compile:coffee": "./node_modules/coffee-script/bin/coffee --compile --output ./lib ./src/coffeescripts",
    "compile:ts": "./node_modules/typescript/bin/tsc --outDir ./lib --module commonjs ./src/typescripts/tsCode.ts",
    "compile": "npm run compile:coffee && npm run compile:ts",
    "clean": "rm -rf lib/*"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": [
    "npm",
    "build"
  ],
  "author": "Marcus Hammarberg @marcusoftnet",
  "license": "MIT"
}
